{
  "name": "deelbreaker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "db:create": "npx tsx scripts/create-database.ts",
    "db:test": "npx tsx scripts/test-db-connection.ts",
    "db:setup": "powershell -ExecutionPolicy Bypass -File scripts/setup-db.ps1",
    "env:validate": "node scripts/validate-env.js",
    "env:check": "node scripts/validate-env.js",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:seed": "dotenv -e .env.local -- npx tsx prisma/seed.ts",
    "db:verify": "npx tsx scripts/verify-migration.ts"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/third-parties": "^16.1.1",
    "@panva/hkdf": "^1.2.1",
    "@prisma/client": "^5.15.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "daisyui": "^5.5.14",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.25.0",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "minio": "^8.0.6",
    "mobileui": "^2.0.4",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.12",
    "pg": "^8.16.3",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "redis": "^5.10.0",
    "regenerator-runtime": "^0.14.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "stripe": "^14.21.0",
    "uuid": "^13.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.5",
    "@types/pg": "^8.11.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "prisma": "^5.15.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
